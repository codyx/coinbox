{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "global-require": 0,
        "react/state-in-constructor": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "import/prefer-default-export": 0,
        "react/jsx-one-expression-per-line": 0
    }
}